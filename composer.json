{
    "name": "comquer/comquer",
    "description": "Event Sourced PHP Framework",
    "type": "library",
    "authors": [
        {
            "name": "Jakub Giminski",
            "email": "jakubgiminski@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Comquer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Comquer\\Test\\": "test/",
            "Comquer\\TestVendor\\": "test/vendor/"
        }
    },
    "require": {
        "comquer/collection": "dev-master",
        "psr/container": "^1.0@dev",
        "psr/http-message": "dev-master",
        "php-di/php-di": "dev-master",
        "comquer/persistence": "dev-master",
        "comquer/reflection": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "comquer/validator": "dev-master"
    }
}
